{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "quotes": [2, "single", { "avoidEscape": true }],
    "no-unused-vars": "off",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "semi": [2, "always"],
    "indent": ["error", 2]
  },
  "globals": {
    "globalThis": "readonly",
    "React": "readonly"
  }
}